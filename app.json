{
  "expo": {
    "icon": "./assets/logo.png",
    "name": "Bible Alias",
    "slug": "allias",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "bible.alias",
      "newArchEnabled": true
    },
    "android": {
      "package": "bible.alias",
      "icon": "./assets/logo.png",
      "newArchEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "33764b1d-a25d-4a4e-824b-ca446adf3bcb"
      }
    },
    "owner": "nktssfrn",
    "plugins": [
    ]
  }
}